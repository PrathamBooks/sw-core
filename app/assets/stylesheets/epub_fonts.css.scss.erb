@font-face {
	font-family:'Noto Sans';
	src: url('fonts/Noto_Sans/Regular/NotoSans_gdi.eot');
	src: url('fonts/Noto_Sans/Regular/NotoSans_gdi.eot?#iefix') format('embedded-opentype'),
		url('fonts/Noto_Sans/Regular/NotoSans_gdi.woff') format('woff'),
		url('fonts/Noto_Sans/Regular/NotoSans_gdi.ttf') format('truetype'),
		url('fonts/Noto_Sans/Regular/NotoSans_gdi.svg#NotoSans-Regular') format('svg');
	font-weight: 400;
	font-style: normal;
	font-stretch: normal;
	unicode-range: U+000D-2666;
}
@font-face {
	font-family:'Noto Sans';
	src: url('fonts/Noto_Sans/Italic/NotoSans-Italic_gdi.eot');
	src: url('fonts/Noto_Sans/Italic/NotoSans-Italic_gdi.eot?#iefix') format('embedded-opentype'),
		url('fonts/Noto_Sans/Italic/NotoSans-Italic_gdi.woff') format('woff'),
		url('fonts/Noto_Sans/Italic/NotoSans-Italic_gdi.ttf') format('truetype'),
		url('fonts/Noto_Sans/Italic/NotoSans-Italic_gdi.otf') format('opentype'),
		url('fonts/Noto_Sans/Italic/NotoSans-Italic_gdi.svg#NotoSans-Italic') format('svg');
	font-weight: 400;
	font-style: italic;
	font-stretch: normal;
	unicode-range: U+000D-2666;
}
@font-face {
	font-family:'Noto Sans';
	src: url('fonts/Noto_Sans/Bold/NotoSans-Bold_gdi.eot');
	src: url('fonts/Noto_Sans/Bold/NotoSans-Bold_gdi.eot?#iefix') format('embedded-opentype'),
		url('fonts/Noto_Sans/Bold/NotoSans-Bold_gdi.woff') format('woff'),
		url('fonts/Noto_Sans/Bold/NotoSans-Bold_gdi.ttf') format('truetype'),
		url('fonts/Noto_Sans/Bold/NotoSans-Bold_gdi.otf') format('opentype'),
		url('fonts/Noto_Sans/Bold/NotoSans-Bold_gdi.svg#NotoSans-Bold') format('svg');
	font-weight: 700;
	font-style: normal;
	font-stretch: normal;
	unicode-range: U+000D-2666;
}
@font-face {
	font-family:'Noto Sans';
	src: url('fonts/Noto_Sans/BoldItalic/NotoSans-BoldItalic_gdi.eot');
	src: url('fonts/Noto_Sans/BoldItalic/NotoSans-BoldItalic_gdi.eot?#iefix') format('embedded-opentype'),
		url('fonts/Noto_Sans/BoldItalic/NotoSans-BoldItalic_gdi.woff') format('woff'),
		url('fonts/Noto_Sans/BoldItalic/NotoSans-BoldItalic_gdi.ttf') format('truetype'),
		url('fonts/Noto_Sans/BoldItalic/NotoSans-BoldItalic_gdi.otf') format('opentype'),
		url('fonts/Noto_Sans/BoldItalic/NotoSans-BoldItalic_gdi.svg#NotoSans-BoldItalic') format('svg');
